<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="numberController" class="com.pengurus.crm.server.GwtRpcController">
		<property name="remoteService" ref="numberServiceBean" />
	</bean>
	
	<bean id="userController" class="com.pengurus.crm.server.GwtRpcController">
		<property name="remoteService" ref="userServiceBean" />
	</bean>
	
	<bean id="currentSessionController" class="com.pengurus.crm.server.GwtRpcController">
		<property name="remoteService" ref="currentSessionServiceBean" />
	</bean>
	
	<bean id="quoteController" class="com.pengurus.crm.server.GwtRpcController">
		<property name="remoteService" ref="quoteServiceBean" />
	</bean>
	
</beans>